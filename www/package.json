{
  "name": "docs-root",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ]
  },
  "scripts": {
    "build": "turbo run build",
    "build:docs": "turbo run build --filter=docs",
    "start": "turbo run start:monorepo",
    "dev": "turbo run dev:monorepo",
    "lint": "turbo run lint",
    "lint:content": "turbo run lint:content"
  },
  "dependencies": {
    "autoprefixer": "10.4.18",
    "eslint": "^8.57.0",
    "postcss": "8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "3.4.1",
    "tsconfig": "*",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "resolutions": {
    "string-width": "4.2.3",
    "jackspeak": "2.1.1"
 }
}
