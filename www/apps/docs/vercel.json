{
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/",
      "has": [ { "type": "host", "value": "min.arg.sh" } ],
      "destination": "https://raw.githubusercontent.com/arg-sh/argsh/main/argsh.min.sh"
    },
    {
      "source": "/",
      "has": [ { "type": "host", "value": "test.arg.sh" } ],
      "destination": "https://raw.githubusercontent.com/arg-sh/argsh/main/test/helper.bash"
    },
    {
      "source": "/",
      "has": [ { "type": "host", "value": "get.arg.sh" } ],
      "destination": "https://raw.githubusercontent.com/arg-sh/argsh/main/bootstrap/install.sh"
    },
    {
      "source": "/",
      "has": [ { "type": "host", "value": "envrc.arg.sh" } ],
      "destination": "https://raw.githubusercontent.com/arg-sh/argsh/main/bootstrap/.envrc"
    },
    {
      "source": "/",
      "has": [ { "type": "host", "value": "github.arg.sh" } ],
      "destination": "https://raw.githubusercontent.com/arg-sh/argsh/main/bootstrap/.github/workflows/argsh.yaml"
    },
    {
      "source": "/",
      "has": [ { "type": "host", "value": "tmpl.arg.sh" } ],
      "destination": "https://raw.githubusercontent.com/arg-sh/argsh/main/bootstrap/scripts/main.sh"
    },
    {
      "source": "/",
      "has": [ { "type": "host", "value": "tmpl-test.arg.sh" } ],
      "destination": "https://raw.githubusercontent.com/arg-sh/argsh/main/bootstrap/scripts/main.bats"
    }
  ],
  "framework": "docusaurus-2",
  "installCommand": "yarn install",
  "buildCommand": "turbo run build",
  "outputDirectory": "build",
  "ignoreCommand": "bash ../../ignore-build-script.sh",
  "devCommand": "yarn dev"
}